{
	"name": "ancelot",
	"colors": {
		// editor
		//"editor.background": "#263238", // green
		//"editor.background": "#282c34", // gray lighter
		//"editor.background": "#212835", // gray darker
		"editor.background": "#1b212c",  // gray darker +
		//"editor.background": "#0c1821",  // gray darker ++
		//"editor.background": "#212529",  // gray darker -
		//"editor.background": "#151922",  // gray darker + terminal
		"editor.foreground": "#adb5bd",

		// editor find and selection
		"editor.findMatchBackground": "#f8f9fa",
		"editor.findMatchForeground": "#404246",
		"editor.findMatchHighlightBackground": "#f8f9fa",
		"editor.findMatchHighlightForeground": "#1d1d1f",

		"editor.lineHighlightBackground": "#2c313a",
		"editor.selectionBackground": "#2c313a",

		// cursor
		"editorCursor.foreground": "#528bff",

		// "editor.selectionBackground": "#3e4451",
		// "editorCursor.foreground": "#528bff",
		// "editor.selectionForeground": "#404246",
		// "editor.selectionHighlightBackground": "#1b212c",
		// "editor.lineHighlightBackground": "#2c313a",

		// editor line number
		"editorLineNumber.activeForeground": "#cdcecd",
		"editorLineNumber.foreground": "#5c5e5d80",

		// list
		"list.activeSelectionBackground": "#f7f8fe",
		"list.activeSelectionForeground": "#303633",
		"list.hoverBackground": "#f7f8fe",
		"list.hoverForeground": "#303633",
		"list.inactiveSelectionBackground": "#f7f8fe",
		"list.inactiveSelectionForeground": "#303633",

		// brackets
		"editorBracketMatch.background": "#858585",
		"editorBracketMatch.border": "#00000000",
		"editorBracketHighlight.foreground1": "#c2cad3",
		"editorBracketHighlight.foreground2": "#c2cad3",
		"editorBracketHighlight.foreground3": "#c2cad3",
		"editorBracketHighlight.foreground4": "#c2cad3",
		"editorBracketHighlight.foreground5": "#c2cad3",
		"editorBracketHighlight.foreground6": "#c2cad3",

